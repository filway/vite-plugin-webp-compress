{
  "name": "vite-plugin-webp-compress",
  "version": "1.0.0",
  "description": "A Vite plugin for compressing and converting images to WebP format during the build process.",
  "keywords": [
    "vite",
    "vite-plugin",
    "image",
    "webp",
    "compression",
    "optimization",
    "build",
    "webpack",
    "frontend",
    "development"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "test": "vitest",
    "dev:example": "vite dev example",
    "build:example": "vite build example",
    "preview:example": "vite preview example",
    "build": "tsup"
  },
  "author": "filway <zhouxiaocr@gmail.com>",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.1.0",
    "@mistjs/tsconfig-vue": "^1.1.2",
    "@types/node": "^20.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "eslint": "^8.50.0",
    "lint-staged": "^14.0.1",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.38",
    "vitest": "^0.34.5",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
